{
  "expo": {
    "name": "e-aluguel",
    "slug": "e-aluguel",
    "version": "1.0.0",
    "web": {},
    "experiments": {
      "tsconfigPaths": true
    },
    "plugins": [
      ["onesignal-expo-plugin", {
        "mode": "development"
      }]
    ],
    "orientation": "portrait",
    "splash": {
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "icon": "./src/assets/icon.png",
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.victor.rayan.ealuguel"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./src/assets/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      },
      "package": "com.victor.rayan.ealuguel"
    },
    "extra": {
      "eas": {
        "projectId": "4fdc05bb-6e18-45bf-9b01-38d56b59f893"
      }
    }
  }
}
